-editor.tsx",["109"],"import MonacoEditor, { monaco, EditorDidMount } from \"@monaco-editor/react\";\nimport prettier from \"prettier\";\nimport parser from \"prettier/parser-babel\";\nimport { useRef } from \"react\";\nimport \"./code-editor.css\";\nimport codeShift from \"jscodeshift\";\nimport Highlighter from \"monaco-jsx-highlighter\";\nimport \"./syntax.css\";\n\ninterface CodeEditorProps {\n  initialValue: string;\n  onChange(value: string): void;\n}\n\nconst CodeEditor: React.FC<CodeEditorProps> = ({ onChange, initialValue }) => {\n  const editorRef = useRef<any>();\n\n  const editorDidMount: EditorDidMount = (getValue, monacoEditor) => {\n    editorRef.current = monacoEditor;\n    monacoEditor.onDidChangeModelContent(() => {\n      onChange(getValue());\n    });\n\n    monacoEditor.getModel()?.updateOptions({ tabSize: 2 });\n\n    const highlighter = new Highlighter(\n      // @ts-ignore\n      window.monaco,\n      codeShift,\n      monacoEditor\n    );\n    highlighter.highLightOnDidChangeModelContent(\n      () => {},\n      () => {},\n      undefined,\n      () => {}\n    );\n  };\n  const onFormatClick = () => {\n    // get current value\n    const unformattd = editorRef.current.getModel().getValue();\n    //format\n\n    const formattd = prettier\n      .format(unformattd, {\n        parser: \"babel\",\n        plugins: [parser],\n        useTabs: false,\n        semi: true,\n        singleQuote: true,\n      })\n      .replace(/\\n$/, \"\");\n    //set formatted val back in the editor\n    editorRef.current.setValue(formattd);\n  };\n  return (\n    <div className=\"editor-wrapper\">\n      <button\n        className=\"button button-format is-primary is-small\"\n        onClick={onFormatClick}\n      >\n        Format meeee\n      </button>\n      <MonacoEditor\n        editorDidMount={editorDidMount}\n        value={initialValue}\n        theme=\"dark\"\n        language=\"javascript\"\n        height=\"100%\"\n        options={{\n          wordWrap: \"on\",\n          scrollBeyondLastLine: true,\n          minimap: { enabled: false },\n          showUnused: false,\n          folding: false,\n          lineNumbersMinChars: 2,\n          fontSize: 16,\n          automaticLayout: true,\n        }}\n      />\n    </div>\n  );\n};\n\nexport default CodeEditor;\n","/Users/vaughnohlerking/Desktop/CrimerDev/training/buildAPortfolio/jbook/packages/local-client/src/state/reducers/bundlesReducer.ts",[],"/Users/vaughnohlerking/Desktop/CrimerDev/training/buildAPortfolio/jbook/packages/local-client/src/state/reducers/cellsReducer.ts",[],"/Users/vaughnohlerking/Desktop/CrimerDev/training/buildAPortfolio/jbook/packages/local-client/src/state/action-types/index.ts",[],"/Users/vaughnohlerking/Desktop/CrimerDev/training/buildAPortfolio/jbook/packages/local-client/src/bundler/index.ts",[],"/Users/vaughnohlerking/Desktop/CrimerDev/training/buildAPortfolio/jbook/packages/local-client/src/bundler/plugins/unpkg-path-plugin.ts",[],"/Users/vaughnohlerking/Desktop/CrimerDev/training/buildAPortfolio/jbook/packages/local-client/src/bundler/plugins/fetch-plugin.ts",[],"/Users/vaughnohlerking/Desktop/CrimerDev/training/buildAPortfolio/jbook/packages/local-client/src/state/middlewares/persist-middleware.ts",[],{"ruleId":"110","severity":1,"message":"111","line":15,"column":23,"nodeType":"112","messageId":"113","endLine":15,"endColumn":32},{"ruleId":"114","severity":1,"message":"115","line":19,"column":6,"nodeType":"116","endLine":19,"endColumn":8,"suggestions":"117"},{"ruleId":"118","replacedBy":"119"},{"ruleId":"120","replacedBy":"121"},{"ruleId":"110","severity":1,"message":"122","line":1,"column":24,"nodeType":"112","messageId":"113","endLine":1,"endColumn":30},"@typescript-eslint/no-unused-vars","'saveCells' is assigned a value but never used.","Identifier","unusedVar","react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'fetchCells'. Either include it or remove the dependency array.","ArrayExpression",["123"],"